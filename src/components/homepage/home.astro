---
import Layout from "../../layouts/Layout.astro";
---

<!-- outer-->
<div class="outer">
  <!-- header-->
  <!-- container-->
  <div class="outer__content">
    <div class="main">
      <div class="main__center center">
        <div class="main__wrap">
          <div class="stage">TRAIN. COMPETE. REPRESENT KENYA.</div>
          <h1 class="main__title h1">
            Kenya's Multi-Disciplinary Sports Academy
          </h1>
          <div class="main__text">
            Empowering Kenyan athletes for success at national and international
            levels
          </div>
          <div class="main__btns">
            <a class="button main__button" href="/contact">Explore Programs</a
            ><a class="button-stroke main__button" href="membership.html"
              >Join Now</a>
          </div>
        </div>
        <a class="scroll js-scroll" href="#steps">
          <div class="scroll__line">
            <svg class="icon icon-arrow-down">
              <use xlink:href="#icon-arrow-down"></use>
            </svg>
            <svg class="icon icon-arrow-down">
              <use xlink:href="#icon-arrow-down"></use>
            </svg>
          </div>
        </a>
        <div class="main__gallery">
          <div
            class="main__preview"
            data-aos="animation-translate-up"
            data-aos-anchor=".main__gallery"
          >
            <img
              class="some-icon"
              src="/img/content/avatar-8.png"
              alt="Watch"
            /><img
              class="some-icon-dark"
              src="/img/content/avatar-8.png"
              alt="Watch"
            />
          </div>
          <div
            class="main__preview"
            data-aos="animation-translate-up"
            data-aos-delay="600"
            data-aos-anchor=".main__gallery"
          >
            <img src="/img/content/ball.png" alt="Ball" />
          </div>
          <div
            class="main__preview"
            data-aos="animation-translate-up"
            data-aos-delay="300"
            data-aos-anchor=".main__gallery"
          >
            <img src="/img/content/bottle.png" alt="Bottle" />
          </div>
          <div
            class="main__preview"
            data-aos="animation-translate-up"
            data-aos-delay="900"
            data-aos-anchor=".main__gallery"
          >
            <img src="/img/content/ball-black.png" alt="Ball black" />
          </div>
        </div>
      </div>
    </div>
